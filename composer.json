{
    "name": "ctsmedia/contao-phpbb-bridge-bundle",
    "description": "phpbb 3.1 / 3.2 Bridge for Contao 4 Standard and Managed Edition",
    "type": "contao-bundle",
    "keywords": [
        "contao",
        "phpbb",
        "symfony",
        "forum"
    ],
    "license": "GPL-2.0-only",
    "authors": [
        {
            "name": "Daniel Schwiperich",
            "email": "d.schwiperich@cts-media.eu"
        }
    ],
    "require": {
        "php": "^5.6|^7.0",
        "contao/core-bundle": "^4.0.4",
        "kriswallsmith/buzz": "^0.15"
    },
    "require-dev": {
        "contao/managed-edition": "^4.4",
        "contao/manager-plugin": "^2.0"
    },
    "conflict": {
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "autoload": {
        "psr-4": {
            "Ctsmedia\\Phpbb\\BridgeBundle\\": "src/"
        }
    },
    "extra": {
        "contao-manager-plugin": "Ctsmedia\\Phpbb\\BridgeBundle\\ContaoManager\\Plugin"
    }
}
